@html.extend('layout', function(model){
    @html.block('body', function(model){
        <h1>@model.title</h1>
        @if (model.error) {
            <p class="text-error">Error ocurred: @model.error</p>
        }
        
        @model.categories.forEach(function (category) {
            <div><a href="#">@category.name</a></div>
        })
    });
})